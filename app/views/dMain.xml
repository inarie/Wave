<Alloy>
	<Window id="dMain" class="container" onOpen="go_back" onFocus="loadProperties">
		<ActionBar id="actionbar" title="Main"/>
		<TableView id="table">
			<TableViewSection>
				<TableViewRow class="row" onClick="goToPersons">
					<Label id="persons" class="label">Persons:</Label>
				</TableViewRow>
				<TableViewRow class="row" onClick="goToMap">
					<Label id="location" class="label">Location:</Label> 
				</TableViewRow>
				<TableViewRow class="row" onClick="goToDuration">
					<Label id="duration" class="label">Duration:</Label>
				</TableViewRow>
			</TableViewSection>
		</TableView>
	</Window>
</Alloy>