			<TableView id="table" onClick="openSurvey">
				<TableViewSection id="row">
					<TableViewRow class="row" id="persons" title="Persons">
					</TableViewRow>
					<TableViewRow class="row" id="location" title="Company"> 
					</TableViewRow>
					<TableViewRow class="row" id="duration" tittle="Time">
					</TableViewRow>
				</TableViewSection>
			</TableView>
			<Button id="next" onClick="openMap"></Button>
	</Window>
</Alloy>